{
  "expo": {
    "name": "Delivery Oba",
    "description": "E-commerce app for Delivery Oba",
    "slug": "deliveryoba",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#049434",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#049434"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.deliveryoba.deliveryoba",
      "buildNumber": "1.0.0",
      "usesIcloudStorage": false,
      "usesAppleSignIn": false,
      "accessesContactNotes": false
    },
    "android": {
      "package": "com.deliveryoba.deliveryoba",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_fg.png",
        "backgroundColor": "#049434"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ] 
    }
  }
}
